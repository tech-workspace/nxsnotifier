{
  "expo": {
    "scheme": "nxsnotifier",
    "name": "Nexus Notifier",
    "slug": "nxsnotifier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo-2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/logo-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techworkspace.nxsnotifier",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.techworkspace.nxsnotifier",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo-2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/img/logo-2.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e15d0b22-5471-4ae1-825c-5e60d723b70b"
      }
    }
  }
}